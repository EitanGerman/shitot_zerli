<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER_LEFT" prefHeight="61.0" prefWidth="1170.0" spacing="10.0" style="-fx-background-color: D8E2DC; -fx-background-radius: 50;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="surveyAnalysisView.AnalysisViewRowController">
   <children>
      <Label text="Survery ID:">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Label fx:id="surveryIDNumber" minWidth="100.0" text="id Number">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Separator orientation="VERTICAL" prefHeight="200.0" />
      <Label text="Survery Name:">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Label fx:id="surveyNameLabel" prefHeight="60.0" prefWidth="565.0" text="name" wrapText="true">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="viewAnalysisButton" mnemonicParsing="false" onAction="#openReportAnalysis" style="-fx-background-radius: 20;" text="View Analysis Report">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
   <padding>
      <Insets left="30.0" />
   </padding>
</HBox>
