<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="540.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="catalog.CustomProductOverviewController">
   <children>
      <HBox alignment="CENTER" prefHeight="46.0" prefWidth="804.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="69.0" prefWidth="565.0" text="Custom Product Overview" textAlignment="CENTER">
               <font>
                  <Font size="47.0" />
               </font>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <ScrollPane prefHeight="386.0" prefWidth="894.0">
         <content>
            <VBox fx:id="cartItemVBox" prefHeight="402.0" prefWidth="844.0" />
         </content>
      </ScrollPane>
      <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="51.0">
         <children>
            <Button fx:id="backToCatalogButton" mnemonicParsing="false" onMousePressed="#changeToCatalogScreen" prefHeight="27.0" prefWidth="49.0" text="Back">
               <HBox.margin>
                  <Insets right="349.0" />
               </HBox.margin>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <VBox alignment="CENTER" prefHeight="99.0" prefWidth="173.0" spacing="5.0">
               <children>
                  <Label text="Product Price">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <Label fx:id="priceLabel" text="59 NIS">
                     <font>
                        <Font size="34.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
            <Button fx:id="buyButton" mnemonicParsing="false" onMousePressed="#buy" prefHeight="25.0" prefWidth="89.0" text="Add To Cart">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
               <padding>
                  <Insets right="2.0" />
               </padding>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets left="13.0" right="13.0" />
         </padding>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
   </padding>
</VBox>
